<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %> <br> <br> <br> <br> <br>
         <br>Name: <%= @user.name %>
         <br>Team Name: <%= @user.team_name %>
         <br>Email: <%= @user.email %>
         <br>Phone Number: <%= @user.phone_number %>
         <br>Age: <%= @user.age %>
      </h1>
    </section>
  </aside>
</div>